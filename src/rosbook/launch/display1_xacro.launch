<?xml version="1.0" encoding="UTF-8" ?>
<launch>
    <arg name="model"/>
    <arg name="gui" default="False"/>
    <param name="robot_description" command="$(find xacro)/xacro $(arg model)"/>
    <node pkg="rosbook" type="state_publisher_tutorials"/>
    <node pkg="rosbook" type="state_publisher"/>
    <node pkg="rviz" type="rviz" args="-d $(find rosbook)/urdf.rviz"/>
</launch>